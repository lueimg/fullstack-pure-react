{
  "name": "advreact",
  "version": "1.0.0",
  "description": "adv react",
  "main": "lib/server.js",
  "repository": "https://github.com/lueimg/fullstack-pure-react.git",
  "author": "luis <luis@luiss-MacBook-Pro.local>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node",
    "webpack": "webpack -wd",
    "test": "jest --watch",
    "verify-test": "jest --coverage",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files"
  },
  "babel": {
    "presets": [
      "react",
      ["env", {
        "target": {
          "node": "current"
        }
      }]
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
  },
  "devDependencies": {
    "axios": "^0.16.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-plugin-react": "^7.1.0",
    "jest": "^20.0.4",
    "react-addons-perf": "^15.4.2",
    "react-test-renderer": "^15.6.1",
    "styled-components": "^2.1.1"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^2.6.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack": "^3.4.0"
  }
}
